{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        ["emotion", { "sourceMap": true }],
        "@babel/plugin-syntax-dynamic-import",
        "@loadable/babel-plugin"
    ],
    "env": {
        "development": {
            "sourceMaps": true,
            "retainLines": true
        }
    }
}
